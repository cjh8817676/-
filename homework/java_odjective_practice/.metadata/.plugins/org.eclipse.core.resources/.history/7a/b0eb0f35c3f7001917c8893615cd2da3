class Car
{
	//Car 摸O
	private int num;  //~场Lk奖郸s@
	private double gas;
	
	public Car()  //QIs韩睾cΑ]睾cl^
	{
		num = 0;
		gas = 0.0;
		System.out.println("ネ玻Fól");
	}
	public Car(int n, double g)  //QIs韩睾cΑ]睾cl^
	{
		num = 0;
		gas = 0.0;
		System.out.println("ネ玻Fó"+num+"Toq"+gas+"酣l");
	}
	
	public void show()
	{
		System.out.println("ó腹O:"+ this.num);
		System.out.println("Tóoq:"+ this.gas);
	}
}