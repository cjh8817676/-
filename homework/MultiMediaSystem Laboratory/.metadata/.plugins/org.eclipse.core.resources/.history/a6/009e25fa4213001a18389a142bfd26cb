
public class Sample6_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
class Car extends Thread{
	private String name;
	
	public Car(String nm)
	{
		name = nm;
	}
	
	public void run()
	{
		for(int i=0;i<5;i++)
		{
			try
			{
				sleep(1000);
				System.out.println("正在進行"+ name + "的處裡工作");
			}
			catch(InterruptedException e)
			{
				
			}
			
		}
		
	}

}